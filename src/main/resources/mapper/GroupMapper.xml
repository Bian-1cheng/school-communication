<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.commucation.demo.mapper.GroupMapper">
    <resultMap id="GroupResultMap" type="com.commucation.demo.entity.Group">
        <result column="Id" property="id" />
    </resultMap>
    <update id="changHead">
        update course_group
        set head = #{dir}
        where Id = #{id}
    </update>
    <update id="changeGroupName">
        update course_group
        set group_name = #{group_name}
        where Id = #{id}
    </update>
</mapper>